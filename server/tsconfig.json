{
  "compilerOptions": {
    "declaration": true,
    "baseUrl": "..",
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "sourceMap": true,
    "outDir": "../server_build"
  },
  "include": [
    "index.ts",
    "../@types"
  ],
  "exclude": []
}
