  "scripts": {
    "start": "scripts/set_dev_env.sh && craco start",
    "build": "scripts/set_prod_env.sh && craco build",
    "deploy": "bash scripts/deploy.sh",
    "workflow:deploy": "yarn build && yarn server:build && yarn deploy",
    "server:start": "cross-env NODE_ENV=development nodemon --ext ts,js --watch 'server/index.ts' --ignore './src' --exec ts-node --files --project server/tsconfig.json server/index.ts",
    "server:build": "tsc -p server/tsconfig.json && cp -r server/db/migrations server_build && cp server/db/config.json server_build/server/db/config.json",
    "server:deploy": "bash scripts/deploy.sh -s",
    "test": "craco test",
    "eject": "react-scripts eject",
    "typecheck:all": "yarn front:typecheck && yarn server:typecheck",
    "front:typecheck": "tsc --noEmit -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "server:typecheck": "tsc --noEmit -p server/tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "db:hashgen": "node server/db/scripts/passhashgen.js",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed:all",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "cy:open": "cypress open",
    "cy:run:admin": "cypress run --headed --spec cypress/integration/admin_basic.js",
    "cy:run:visitor": "cypress run --headed --spec cypress/integration/visitor_basic.js",
    "cy:run:headless:admin": "cypress run --spec cypress/integration/admin_basic.js",
    "cy:run:headless:visitor": "cypress run --spec cypress/integration/visitor_basic.js",
    "cy:run:headless:admin:firefox": "cypress run -b firefox --spec cypress/integration/admin_basic.js",
    "cy:run:headless:visitor:firefox": "cypress run -b firefox --spec cypress/integration/visitor_basic.js"
  }
