  "scripts": {
    "start": "craco start",
    "build": "craco build --profile",
    "deploy": "bash deploy.sh",
    "workflow:deploy": "yarn build && yarn server:build && yarn deploy",
    "server:start": "NODE_ENV=development nodemon --ext ts,js --watch 'server.ts' --ignore './src' --exec ts-node --project tsconfig.server.json server.ts",
    "server:build": "tsc -p tsconfig.server.json && cp -r db/migrations server_build && cp db/config.json server_build/db/config.json",
    "server:deploy": "bash deploy.sh -s",
    "test": "craco test",
    "eject": "react-scripts eject",
    "typecheck": "yarn front:typecheck && yarn server:typecheck",
    "front:typecheck": "tsc --noEmit -p tsconfig.json",
    "server:typecheck": "tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "db:hashgen": "node db/scripts/passhashgen.js",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "cypress:run:admin": "cypress run --headed --spec cypress/integration/admin_basic.js",
    "cypress:run:visitor": "cypress run --headed --spec cypress/integration/visitor_basic.js",
    "cypress:run:headless:admin": "cypress run -b --spec cypress/integration/admin_basic.js",
    "cypress:run:headless:visitor": "cypress run --spec cypress/integration/visitor_basic.js",
    "cypress:run:admin:electron": "cypress run -b electron --headed --spec cypress/integration/admin_basic.js",
    "cypress:run:visitor:electron": "cypress run -b electron --headed --spec cypress/integration/visitor_basic.js",
    "cypress:run:headless:admin:electron": "cypress run -b electron --spec cypress/integration/admin_basic.js",
    "cypress:run:headless:visitor:electron": "cypress run -b electron --spec cypress/integration/visitor_basic.js"
  }
}
